    "three-orbit-controls": "^72.0.0",
    "vue": "^2.5.11",
    "vue-fullscreen": "^2.1.3",
    "vue-google-oauth": "github:TinyNova/vue-google-oauth",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.1.6"
